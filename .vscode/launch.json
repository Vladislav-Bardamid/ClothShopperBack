{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Watch",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "watch",
      "program": "${workspaceFolder}/ClothShopperBack.API/bin/Debug/net7.0/ClothShopperBack.API.dll",
      "args": [],
      "cwd": "${workspaceFolder}/ClothShopperBack.API",
      "stopAtEntry": false,
      "serverReadyAction": {
        "action": "openExternally",
        "pattern": "\\bNow listening on:\\s+(https?://\\S+)",
        "uriFormat": "%s/swagger/index.html"
